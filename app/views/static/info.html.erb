                                <%= image_tag "ruby icon color", :class => "iconimg" %>
                                <div class="moj_info_item">
                                    <div class="name">
                                        Ruby
                                    </div>
                                    <div class="text"><%= t "InfoPage.ruby" %></div>
                                    <div class="more">
                                        <a href="https://www.ruby-lang.org/en/" class="btn btn-default btn-sm" target ="_blank">Read more</a>
                                    </div>
                                </div>
                            </div>
                        </div>
